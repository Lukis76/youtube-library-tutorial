{
  "$schema": "https://json.schemastore.org/prettierrc.json",
  "tabWidth": 2,
  "printWidth": 100,
  "semi": false,
  "useTabs": false,
  "singleQuote": true,
  "parser": "flow",
  "endOfLine": "auto",
  "arrowParens": "always",
  "trailingComma": "none",
  "bracketSameLine": true,
  "bracketSpacing": true
}